@model IEnumerable<БИПиТ9.Client>

@{
    ViewBag.Title = "Client";
}

<h2>Клиенты</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Client1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Client1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
    </tr>
}

</table>
