@model IEnumerable<БИПиТ9.Service>

@{
    ViewBag.Title = "Service";
}

<h2>Услуги</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Service1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Service1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
    </tr>
}

</table>
